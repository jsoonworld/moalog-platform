# ======================
# 통합 docker-compose 환경변수
# ======================

# --- MySQL (moalog DB) ---
MYSQL_ROOT_PASSWORD=your_mysql_password
MYSQL_DATABASE=retrospect

# --- PostgreSQL (fluxpay DB) ---
POSTGRES_USER=fluxpay
POSTGRES_PASSWORD=your_postgres_password
POSTGRES_DB=fluxpay

# --- Redis ---
REDIS_PASSWORD=your_redis_password

# --- moalog-server ---
MOALOG_DATABASE_URL=mysql://root:your_mysql_password@mysql:3306/retrospect
MOALOG_JWT_SECRET=your_jwt_secret
MOALOG_OPENAI_API_KEY=your_openai_key
MOALOG_ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173

# --- Grafana ---
GF_SECURITY_ADMIN_PASSWORD=admin
